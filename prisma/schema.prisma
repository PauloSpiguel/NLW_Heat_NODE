datasource db {
  provider = "sqlite"
  url      = "file:../database/db.sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String @id @default(cuid())
  name       String
  login      String
  github_id  Int
  avatar_url String

  created_at DateTime @default(now())

  @@map("users")
}
